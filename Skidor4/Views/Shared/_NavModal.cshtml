<!-- Modal -->
<div class="modal fade" id="logout-modal" tabindex="-1" role="dialog" aria-labelledby="logoutModalLabel">
    <div class="modal-dialog logout-modal-dialog" role="document">
        <div class="modal-content logout-modal-content">
            <div class="modal-header logout-modal-header">
                <h5 class="modal-title" id="logoutModalLabel">Logga ut</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body logout-modal-body">
                <p>Är du säker på att du vill logga ut?</p>
            </div>
            <div class="modal-footer logout-modal-footer">
                <a class="btn btn-primary" href="@Url.Action("Logout", "Home")">Logga ut</a>
                <a class="btn btn-danger" href="" data-dismiss="modal">Avbryt</a>
            </div>
        </div>
    </div>
</div>
